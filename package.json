{
  "name": "sttlment",
  "version": "1.0.0",
  "description": "نظام إدارة المهام والحضور - قسم التسويات والمطابقة - شركة الصاقي للدفع الإلكتروني",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "migrate": "node server/migrations/runMigrations.js",
    "seed": "node server/migrations/seed.js",
    "seed:comprehensive": "node server/migrations/seed-comprehensive.js",
    "seed:full": "node server/migrations/seed-full.js",
    "reset": "node server/migrations/reset-database.js",
    "reset-seed": "npm run reset && npm run seed",
    "reset-seed:comprehensive": "npm run reset && npm run seed:comprehensive",
    "reset-seed:full": "npm run reset && npm run seed:full",
    "kill-port-5000": "lsof -ti:5000 | xargs kill -9 2>/dev/null; lsof -ti:5001 | xargs kill -9 2>/dev/null; true"
  },
  "keywords": [
    "settlements",
    "tasks",
    "attendance"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment-timezone": "^0.5.43",
    "multer": "^2.0.0",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.13.0",
    "pg": "^8.11.3",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
